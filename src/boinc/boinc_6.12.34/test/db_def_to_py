#!/usr/bin/env perl

## $Id: db_def_to_py 1528 2003-06-18 03:04:06Z quarl $

## parse the db #defines to php.

## syntax: ./db_def_to_php < ../db/boinc_db.h > boinc_db.inc

print "# Generated by db_def_to_py on ";
system('date');

while (<>) {
    if (/^\s*#define\s+([^\s]+)\s+([0-9]+)\s*$/) {
        print qq/$1 = $2\n/;
    }
}
